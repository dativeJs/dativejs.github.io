    "dytejs": "^1.0.1-alpha",
    "highlight.js": "^11.3.1",
    "monaco-editor": "^0.31.1",
    "page": "^1.11.6",
    "pizzle": "^1.0.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.26.0",
